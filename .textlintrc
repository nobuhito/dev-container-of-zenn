{
  "filters": {},
  "rules": {
    "preset-ja-spacing": {
      "ja-space-between-half-and-full-width": {
        "space": "always"
      }
    },
    "preset-ja-technical-writing": {
      "ja-no-mixed-period": {
        "allowPeriodMarks": [
          ":"
        ]
      }
    },
    "@proofdict/proofdict": {
      "dicts": [
        {
          "dictURL": "https://nobuhito.github.io/proof-dictionary/"
        },
        {
          "dictURL": "https://azu.github.io/proof-dictionary/"
        }
      ]
    }
  }
}